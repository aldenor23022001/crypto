nota1 = float(input('digite a primereira nota: '))
nota2 = float(input('digite a segunda nota: '))
me = (nota1 + nota2) / 2
if 6 <= me <= 10:
        print(f'aluno aprovado com media {me}. ')
elif 0 <= me <6:
        print(f'aluno reprovado com media {me}. ')
else:
         print(f'vallor da média inválida {me}. ')    
     


